table = (map (range 10) {_ y ->
  <- (map (range 10) {item x ->
    <- (multiply (add x 1) (add y 1))
  })
})

print_table = {table ->
  (map table {row y ->
    (print (reduce (get row 1 (length row)) {acc item i -> 
      <- (join (string acc) " " (string item))
    } (get row 0)) "\n")
  })
}

(print_table table)